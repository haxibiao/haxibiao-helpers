{
    "name": "haxibiao/helpers",
    "description": "哈希表通用帮助函数和类",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "ivan",
            "email": "ivan@haxibiao.com"
        }
    ],
    "require": {
        "laravel/helpers": "^1.2",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "alipaysdk/easysdk": "^2.0",
        "jenssegers/agent": "^2.6",
        "haxibiao/console": "*"
    },
    "repositories": {
        "nova": {
            "type": "vcs",
            "url": "http://code.haxibiao.cn/web/nova.git"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Haxibiao\\Helpers\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Haxibiao\\Helpers\\HelpersServiceProvider"
            ]
        }
    }
}
